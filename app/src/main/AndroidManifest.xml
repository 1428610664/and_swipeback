<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xbeats.swipebacksample">

    <application
        android:name=".applicationtest.CustomApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.xbeats.swipebacksample.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.xbeats.swipebacksample.slideback.NextActivity"
            android:label="@string/title_activity_next"></activity>

        <activity
            android:name="com.xbeats.swipebacksample.common.CommonActivity"
            android:label="@string/title_activity_common"></activity>

        <activity
            android:name="com.xbeats.swipebacksample.noslide.NoSlideActivity"
            android:label="@string/title_activity_no_slide"
            android:theme="@style/JK.SwipeBack.Transparent.Theme"></activity>

        <activity
            android:name="com.xbeats.swipebacksample.applicationtest.ApplicationTestActivity"
            android:label="ApplicationTestActivity"
            android:theme="@style/TransparentBackgroundTheme.AppTheme"></activity>

        <activity
            android:name="com.xbeats.swipebacksample.copylastactivityview.CopyLastViewActivity"
            android:label="SlideBackActivity"
            android:theme="@style/NoActivityAnimation.AppTheme"/>
        <activity
            android:name="com.xbeats.swipebacksample.flingback.FlingBackActivity"
            android:label="SlideBackActivity"/>
        <activity
            android:name="com.xbeats.swipebacksample.windowmanager.BaseActivity"
            android:label="SlideBackActivity"/>
        <activity
            android:name="com.xbeats.swipebacksample.popwindow.PopupWindowActivity"
            android:label="SlideBackActivity"/>
        <activity
            android:name="com.xbeats.swipebacksample.dispatchactivity.DispatchManagerActivity"
            android:label="SlideBackActivity"/>
        <activity
            android:name="com.xbeats.swipebacksample.multislide.MultiSlideActivity"
            android:label="SlideBackActivity"/>
        <activity
            android:name="com.xbeats.swipebacksample.bitmaptest.BitmapActivity"
            android:label="SlideBackActivity"/>
    </application>

</manifest>
